{
  "name": "cfhelper",
  "displayName": "CF Helper",
  "description": "Codeforces helper",
  "version": "0.0.4",
  "publisher": "psucoder",
  "license": "MIT",
  "engines": {
    "vscode": "^1.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psucodervn/cfhelper"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.parseContest",
        "title": "Parse Contest",
        "category": "CF Helper"
      },
      {
        "command": "extension.login",
        "title": "Login",
        "category": "CF Helper"
      },
      {
        "command": "extension.logout",
        "title": "Logout",
        "category": "CF Helper"
      },
      {
        "command": "extension.submit",
        "title": "Submit current file",
        "category": "CF Helper"
      },
      {
        "command": "extension.setLanguage",
        "title": "Set language",
        "category": "CF Helper"
      },
      {
        "command": "extension.generateSampleTemplates",
        "title": "Generate sample templates",
        "category": "CF Helper"
      }
    ],
    "configuration": {
      "title": "CF Helper configuration",
      "properties": {
        "cfhelper.common": {
          "description": "CF Helper common settings",
          "default": {
            "src": "src",
            "out": "out",
            "tmp": "tmp",
            "language": "cpp14",
            "templates": "templates"
          }
        },
        "cfhelper.languages": {
          "description": "CF Helper languages config map",
          "default": {
            "cpp14": {
              "id": 50,
              "name": "GNU G++14 6.4.0",
              "main": "main",
              "ext": "cpp",
              "template": "main.cpp14.tmpl"
            },
            "py2": {
              "id": 7,
              "name": "Python 2.7",
              "main": "main",
              "ext": "py",
              "template": "main.py2.tmpl"
            },
            "py3": {
              "id": 31,
              "name": "Python 3.6",
              "main": "main",
              "ext": "py",
              "template": "main.py3.tmpl"
            },
            "pypy2": {
              "id": 40,
              "name": "PyPy 2.7 (6.0.0)",
              "main": "main",
              "ext": "py",
              "template": "main.pypy2.tmpl"
            },
            "pypy3": {
              "id": 41,
              "name": "PyPy 3.5 (6.0.0)",
              "main": "main",
              "ext": "py",
              "template": "main.pypy3.tmpl"
            }
          }
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.8",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/node-fetch": "^2.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "node-fetch": "^2.1.2"
  }
}
