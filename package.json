{
  "name": "cfhelper",
  "displayName": "CF Helper",
  "description": "Codeforces helper",
  "author": {
    "name": "psucoder",
    "email": "hungle.info@gmail.com",
    "url": "https://github.com/psucodervn"
  },
  "version": "0.0.8",
  "publisher": "psucoder",
  "license": "MIT",
  "engines": {
    "vscode": "^1.24.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psucodervn/cfhelper"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@types/cheerio": "^0.22.8",
    "@types/mocha": "^2.2.42",
    "@types/node": "^7.0.43",
    "@types/node-fetch": "^2.1.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "node-fetch": "^2.6.1"
  },
  "main": "./out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && NODE_ENV=test node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.parseContest",
        "title": "Parse Contest",
        "category": "CF Helper"
      },
      {
        "command": "extension.login",
        "title": "Login",
        "category": "CF Helper"
      },
      {
        "command": "extension.logout",
        "title": "Logout",
        "category": "CF Helper"
      },
      {
        "command": "extension.submit",
        "title": "Submit current file",
        "category": "CF Helper"
      },
      {
        "command": "extension.setLanguage",
        "title": "Set language",
        "category": "CF Helper"
      },
      {
        "command": "extension.generateSampleTemplates",
        "title": "Generate sample templates",
        "category": "CF Helper"
      },
      {
        "command": "extension.startMonitor",
        "title": "Start submission monitor",
        "category": "CF Helper"
      },
      {
        "command": "extension.stopMonitor",
        "title": "Stop submission monitor",
        "category": "CF Helper"
      }
    ],
    "configuration": {
      "title": "CF Helper configuration",
      "properties": {
        "cfhelper.dirs": {
          "description": "CF Helper directory settings",
          "default": {
            "src": "src",
            "templates": "templates"
          }
        },
        "cfhelper.language": {
          "description": "Programming language to be use in CF Helper",
          "default": "cpp14"
        },
        "cfhelper.autoStartMonitor": {
          "description": "Auto start submission monitor",
          "default": true
        },
        "cfhelper.port": {
          "description": "Listener port",
          "type": "integer",
          "default": 1991
        },
        "cfhelper.languages": {
          "description": "CF Helper languages config map",
          "default": {
            "c": {
              "id": 10,
              "name": "GNU GCC 5.1.0",
              "main": "main",
              "ext": "c",
              "template": "main.c.tmpl"
            },
            "c11": {
              "id": 43,
              "name": "GNU GCC C11 5.1.0",
              "main": "main",
              "ext": "c",
              "template": "main.c11.tmpl"
            },
            "cpp14": {
              "id": 50,
              "name": "GNU G++14 6.4.0",
              "main": "main",
              "ext": "cpp",
              "template": "main.cpp14.tmpl"
            },
            "cpp17": {
              "id": 54,
              "name": "GNU G++17 7.3.0",
              "main": "main",
              "ext": "cpp",
              "template": "main.cpp17.tmpl"
            },
            "cs": {
              "id": 9,
              "name": "C# Mono 5",
              "main": "main",
              "ext": "cs",
              "template": "main.cs.tmpl"
            },
            "go": {
              "id": 32,
              "name": "Go 1.8",
              "main": "main",
              "ext": "go",
              "template": "main.go.tmpl"
            },
            "java": {
              "id": 36,
              "name": "Java 1.8.0_162",
              "main": "Main",
              "ext": "java",
              "template": "main.java.tmpl"
            },
            "kotlin": {
              "id": 48,
              "name": "Kotlin 1.2",
              "main": "main",
              "ext": "kl",
              "template": "main.kl.tmpl"
            },
            "py2": {
              "id": 7,
              "name": "Python 2.7",
              "main": "main",
              "ext": "py",
              "template": "main.py2.tmpl"
            },
            "py3": {
              "id": 31,
              "name": "Python 3.6",
              "main": "main",
              "ext": "py",
              "template": "main.py3.tmpl"
            },
            "pypy2": {
              "id": 40,
              "name": "PyPy 2.7 (6.0.0)",
              "main": "main",
              "ext": "py",
              "template": "main.pypy2.tmpl"
            },
            "pypy3": {
              "id": 41,
              "name": "PyPy 3.5 (6.0.0)",
              "main": "main",
              "ext": "py",
              "template": "main.pypy3.tmpl"
            }
          }
        }
      }
    }
  }
}
